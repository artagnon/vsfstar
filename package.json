{
  "name": "vsfstar",
  "description": "VSCode part of a language server",
  "author": "Ramkumar Ramachandra",
  "publisher": "artagnon",
  "repository": {
    "type": "git",
    "url": "https://github.com/artagnon/vsfstar"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Snippets"
  ],
  "keywords": [
    "FStar",
    "IDE"
  ],
  "activationEvents": [
    "onLanguage:fstar"
  ],
  "main": "out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "fstar configuration",
      "properties": {
        "fstar.path": {
          "type": "string",
          "default": "fstar",
          "description": "The path to fstar executable, e.g.: /usr/bin/fstar"
        },
        "fstar.arguments": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Arguments for fstar server"
        },
        "fstar.syncFileEvents": {
          "type": "boolean",
          "default": true,
          "description": "Whether or not to send file events to fstar (File created, changed or deleted). This can be disabled for performance consideration."
        },
        "fstar.trace": {
          "type": "boolean",
          "default": true,
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "vsfstar.switchheadersource",
        "title": "Switch between Source/Header"
      },
      {
        "command": "vsfstar.activate",
        "title": "Manually activate fstar extension"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.34"
  }
}
